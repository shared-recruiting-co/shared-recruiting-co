{
	"name": "app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "npx prettier --plugin-search-dir . --check . && npx eslint .",
		"format": "npx prettier --plugin-search-dir . --write .",
		"gen-types": "npx supabase gen types typescript --local > src/lib/supabase/types.ts && npm run format"
	},
	"devDependencies": {
		"@playwright/test": "1.39.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.22.3",
		"@tailwindcss/forms": "^0.5.6",
		"@tailwindcss/typography": "^0.5.10",
		"@types/google.accounts": "^0.0.12",
		"@types/js-yaml": "^4.0.5",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.32",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"svelte": "^3.59.2",
		"svelte-check": "^3.7.0",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.0"
	},
	"type": "module",
	"dependencies": {
		"@markdoc/markdoc": "^0.3.2",
		"@neoconfetti/svelte": "^1.0.0",
		"@sentry/node": "^7.80.1",
		"@supabase/auth-helpers-sveltekit": "0.9.4",
		"@supabase/supabase-js": "^2.29.0",
		"@vercel/analytics": "^1.1.1",
		"js-yaml": "^4.1.0"
	}
}
